{
    "name": "openjournalteam/core",
    "description": "Backend for Hosting Panel",
    "keywords": [
        "admin-panel",
        "packages"
    ],
    "homepage": "https://github.com/openjournalteam/core",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Rahman Ramsi",
            "email": "rahmanramsi19@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "shohel/pluggable": "^1.0",
        "nwidart/laravel-modules": "^8.2",
        "spatie/laravel-permission": "^4.2",
        "yajra/laravel-datatables-oracle": "~9.0",
        "hexadog/laravel-themes-manager": "^1.8",
        "livewire/livewire": "^2.5"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OpenJournalTeam\\Core\\": "src",
            "OpenJournalTeam\\Core\\src": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenJournalTeam\\Core\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenJournalTeam\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "OpenJournalTeam\\Core\\CoreFacade",
                "Role": "OpenJournalTeam\\Core\\Models\\Role"
            }
        }
    }
}
